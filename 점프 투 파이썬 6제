# Q1 은 3번째 조건이 출력된다

# Q2
s = 0
i = 1
while i <= 1000:
    if i % 3 == 0:
        s += i
    i += 1
print(s)

# Q3
i = 1
while i <= 5:
    print('*' * i)
    i += 1
    
# Q4
for i in range(1, 101):
    print(i, ',', end='')
    
# Q5
a = [70, 60, 55, 75, 95, 90, 80, 80, 85, 100]
s = 0
for i in a:
    s += i
print(s / 10)

# Q6
numbers = [1, 2, 3, 4, 5]
result = [n*2 for n in numbers if n%2==1]
print(result)
